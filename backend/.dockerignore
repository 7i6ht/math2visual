# Exclude environment files (should be provided at runtime)
.env
.env.local
.env.*.local

# Exclude Python cache and build artifacts
__pycache__/
*.py[cod]
*$py.class
*.so
.Python
*.egg-info/
dist/
build/

# Exclude logs
logs/
*.log

# Exclude git
.git/
.gitignore

# Exclude documentation
docs/
*.md
!README.md

# Exclude test files
tests/
*.test.py
*_test.py

# Exclude IDE files
.vscode/
.idea/
*.swp
*.swo
*~

